{
  "name": "eecsplus",
  "version": "1.0.0",
  "description": "Software engineering prject for group 26",
  "main": "index.html",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "serve build -s -c 1",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint {src,test}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulDavidTucker/EECSPlus.git"
  },
  "keywords": [
    "HTML",
    "CSS",
    "JavaScript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^3.1.3",
    "mkdirp": "^0.5.1"
  },
  "bugs": {
    "url": "https://github.com/PaulDavidTucker/EECSPlus/issues"
  },
  "homepage": "https://github.com/PaulDavidTucker/EECSPlus#readme"
}
